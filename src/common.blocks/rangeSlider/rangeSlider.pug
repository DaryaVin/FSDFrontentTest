mixin rangeSlider(label)
  script(src="https://code.jquery.com/jquery-1.12.4.js")
  script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.js")
  .rangeSlider.colomnContener
    label.rangeSlider__label.spaceBetweenContener #{label}
      input.rangeSlider__amount(type="text", readonly)
    .rangeSlider__slider