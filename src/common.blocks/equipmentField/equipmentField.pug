mixin equipmentField(id)
    +dropdown(id, "").equipmentField.dropdown_theme_field()&attributes(attributes)
      -
          var numberPickerListItems = [
            {value:'bedrooms', label:'Спальни'},
            {value:'beds', label:'Кровати'},
            {value:'bathrooms', label:'Bанные комнаты'}
          ]
      +numberPickerList(id, numberPickerListItems)
